[
    {
        "settings": [ "master" ],
        "specs": {
            "site-layout": {
                "config": {
                    "assets": {
                        "top": {
                            "css": [
                                "/static/demo_news/assets/css/site.css"
                            ]
                        },
                        "bottom": {
                            "js": [
                                "/static/demo_news/assets/js/site.js"
                            ]
                        }
                    },
                    "deploy": true
                },
                "type": "Microframe"
            },
            "home-page": {
                "base": "site-layout",
                "config": {
                    "title": "Home | Mojito App | Prototype",
                    "child": {
                        "config": {
                            "children": {
                                "listing-element": {
                                    "type": "ListingElementMojit"
                                }
                            }
                        },
                        "type": "HomePageMojit"
                    }
                }
            }
        },
        "staticHandling": {
            "cache": false,
            "forceUpdate": true,
            "appName": "demo_news"
