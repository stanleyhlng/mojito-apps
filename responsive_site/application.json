[
    {
        "settings": [ "master" ],
        "appPort": 8666,
        "specs": {
            "base-layout": {
                "config": {
                    "assets": {
                        "top": {
                            "css": [
                                "/static/responsive_site/assets/css/reset.css",
                                "/static/responsive_site/assets/css/bootstrap.css",
                                "/static/responsive_site/assets/css/site.css",
                                "/static/responsive_site/assets/css/site-responsive.css"
                            ]
                        },
                        "bottom": {
                            "js": [
                                "/static/responsive_site/assets/js/site.js"
                            ]
                        }
                    },
                    "deploy": true
                },
                "type": "Microframe"
            },
            "home-page": {
                "base": "base-layout",
                "config": {
                    "title": "Home | Mojito Apps | Prototype",
                    "child": {
                        "config": {
                            "children": {
                                "content-element": {
                                    "type": "ContentElementMojit"
                                },
                                "footer-element": {
                                    "type": "FooterElementMojit"
                                },
                                "header-element": {
                                    "type": "HeaderElementMojit"
                                },
                                "sidebar-element": {
                                    "type": "SidebarElementMojit"
                                }
                            }
                        },
                        "type": "HomePageMojit"
                    }
                }
            },
            "about-page": {
                "base": "base-page",
                "config": {
                    "title": "About | Mojito Apps | Prototype",
                    "child": {
                        "type": "AboutPageMojit"
                    }
                }
            }
        },
        "staticHandling": {
            "appName": "responsive_site"
        }
    },
    {
        "settings": [ "environment:development" ],
        "staticHandling": {
            "forceUpdate": true
        }
    }
]
